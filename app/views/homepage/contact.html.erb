<article id="content" class="cols-b">
<nav id="breadcrumbs">
  <ol>
    <li><%= link_to t('header.start'), root_url %></li>
    <li><%= t('header.contact') %></li>
  </ol>
</nav>
<div class="map-a">
  <header class="vcard">
    <p class="adr"><span class="street-address"><%= t('contact.company') %></span>,<br /><span class="street-address"><%= t('contact.street') %></span>,<br /><span class="locality"><%= t('contact.city') %></span>, <span class="country-name"><%= t('contact.country') %></span></p>
    <ul>
      <li class="tel"><%= t('contact.phone') %></li>
      <li><a class="email">info//pionira-medical/com</a></li>
    </ul>
  </header>
  <div class="map-g"><figure><img src="images/map-<%= params[:locale] %>.png" alt="Placeholder" width="660" height="469"></figure></div>
</div>
<aside>
  <%= form_tag(params, {class: 'contactform', id: 'contact'}) do %>
    <input type="hidden"  name="event" value="start" />
    <fieldset>
      <p>
        <label for="ca">Name</label>
        <input type="text" id="ca" name="form[name]" required>
      </p>
      <p>
        <label for="cb">E-Mail</label>
        <input type="email" id="cb" name="form[email]" required>
      </p>
      <p class="select-b">
        <label for="cc">Subject</label>
        <input name="form[subject]" type="text" id="cd" required>
      </p>
      <p class="select-b">
        <label for="cc">Phone</label>
        <input name="form[phone]" type="text" id="cf" required>
      </p>
      <p>
        <label for="cd">Message</label>
        <textarea id="cc" name="form[message]" required></textarea>
      </p>
      <p><button type="submit">Submit</button></p>
    </fieldset>
  <% end %>
  <form action="/contact" method="post" class="" name="homefrm1" id="">

  </form>
  <div id="alert">
      <div class="message"></div>
  </div>
</aside>
</article>
<script type="text/javascript">
	head.js('http://ajax.googleapis.com/ajax/libs/jquery/1.9.0/jquery.min.js','javascript/scripts.js','javascript/mobile.js')
</script>>
